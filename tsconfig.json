{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",                                   
    "removeComments": true,                              
    "noEmitOnError": true,                               
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "noImplicitAny": true,
    "noImplicitThis": true,                              
    "skipLibCheck": true                                 
  },
  "exclude":[ "node_modules"],
  "include": ["./*"]
}
